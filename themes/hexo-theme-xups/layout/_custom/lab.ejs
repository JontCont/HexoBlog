<!-- 自定义实验室页 -->

<section>
    <img title="图片摄于2017年1月1日深圳梧桐山 By iPhone 7。" src="<%- url_for('/img/lab/banner.jpg?v=3') %>" alt="Lab Banner" style="width: 100%;" alt="Banner" >
</section>
<%
    var projects = page.projects;
    for (var i in projects) {
%>
    <p style="color: #444;">
        <span class="iconfont icon-tags"></span>
        <%- date(projects[i].time, 'YYYY-MM-DD') %>  <%- projects[i].title %>
    </p>

    <ul style="list-style-type: none;">
        <% var content = projects[i].content; %>
        <% for (var j in content) { %>
        <li>
            <a href="<%- content[j].link %>" target="_blank" class="iconfont icon-code fr" title="查看代码" style="color: #ff5e52; border-bottom: none; font-size: 16px; margin-top: 2.5px;"></a>
            <span><%- content[j].name %></span>
        </li>
        <% } %>
    </ul>
<% } %>
