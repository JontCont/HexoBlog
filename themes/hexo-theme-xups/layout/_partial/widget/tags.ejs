<!-- 文章标签 -->
<div class="widget-bd tag-wrap">
  <%
  var tags = site.tags.sort('length', -1);
  tags.forEach(function (tag, index) {
    if (index < 39) {
  %>
    <a class="tag-item" href="<%= url_for(tag.path) %>" title="<%= tag.name %>"><%- tag.name %> (<%- tag.length %>)</a>
  <%
    }
  }); 
  %>
</div>